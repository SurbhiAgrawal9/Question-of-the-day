import java.io.*;
import java.util.*;

class main{
public static void  main (String[] args){

Scanner sc = new Scanner(System.in);
long  n = sc.nextLong();

if(n% 2050 ! =0){
System.out.print("-1");
} else {
long k = n  /2050;
int count  =0;
while(k>0){
count += k % 10;
k / =10;
}
System.out.println(count);
}
}
}
